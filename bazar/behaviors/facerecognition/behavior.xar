                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import time
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.ad=ALProxy("ALAudioDevice")
        self.player=ALProxy("ALAudioPlayer")
        self.playerStop=ALProxy("ALAudioPlayer",True)

    def onLoad(self):
        self.sBip1Id = self.player.loadFile(ALFrameManager.getBehaviorPath(self.behaviorId) + "/bipReco14.wav")
        self.sBip2Id = self.player.loadFile(ALFrameManager.getBehaviorPath(self.behaviorId) + "/bipReco13.wav")
        self.isRecordingLaunched = False
        self.ids=[]

    def onUnload(self):
        self.player.unloadFile(self.sBip1Id)
        self.player.unloadFile(self.sBip2Id)
        self.onInput_onStopRecording()
        self.onInput_onStopPlaying()

    def onInput_onRecord(self):        
        self.nbOfChannels=int(self.getParameter("Number of channels"))
        if (self.nbOfChannels==1) :
            self.fileFormat=".ogg"
        if (self.nbOfChannels==4) :
            self.fileFormat=".wav"
        self.player.play(self.sBip1Id)
        self.ad.startMicrophonesRecording(self.getParameter("Filename"))
        self.isRecordingLaunched = True
        time.sleep(self.getParameter("Duration"))
        self.player.play(self.sBip2Id)        
        self.onInput_onStopRecording()

    def onInput_onStopRecording(self):
        if self.isRecordingLaunched == True :
            self.ad.stopMicrophonesRecording()
            self.isRecordingLaunched = False
            self.onStopped()

    def onInput_onPlay(self):
        self.nbOfChannels=int(self.getParameter("Number of channels"))
        if (self.nbOfChannels==1) :
            self.fileFormat=".ogg"
        if (self.nbOfChannels==4) :
            self.fileFormat=".wav"
        id=self.player.post.playFile(self.getParameter("Filename"))
        self.ids.append(id)
        self.player.wait(id, 0)
        self.ids.remove(id)
        self.onStoppedPlay()

    def onInput_onStopPlaying(self):
        for id in self.ids:
            try:
                self.playerStop.stop(id)
            except:
                pass
        self.onStoppedPlay()]]>
</content>
