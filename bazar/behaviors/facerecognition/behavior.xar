                                                                    <script language="4">
                                                                        <content>
                                                                            <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.timeFilteredResult = [];

    def onUnload(self):
        #puts code for box cleanup here
        pass

    def onInput_onStart(self, p):
        if(len(p) > 0):
            if(len(p[1]) > 0): # just in case of ALValue wrong format
                self.timeFilteredResult = p[1][len(p[1]) -1]
                self.onRecognitionEvent( self.timeFilteredResult )
                if( len(self.timeFilteredResult) == 1 ):
                    if(self.timeFilteredResult[0] == 4):     # DO_YOU_WANT_TO_LEARN_FACE
                        self.onSuggestLearning()
                elif( len(self.timeFilteredResult) == 2 ):
                    if(self.timeFilteredResult[0] == 2):     # ONE_RECOGNISED_FACE
                        self.onRecognizedFaces( self.timeFilteredResult[1] )
                        self.onLearnIfWrong()
                    elif(self.timeFilteredResult[0] == 3):  # SEVERAL_RECOGNISED_FACES
                        for s in self.timeFilteredResult[1]:
                            self.onRecognizedFaces( s )

    def onInput_onStop(self):
        pass]]>
