    def onInput_onLearn(self, p):
        if self.bIsRunning:
            return
        
        # this way of doing (post and wait) is because of a bug with learnface.
        # if we unload the box before it's ended, the method does not end without that.       
        self.bIsRunning = True
        self.learnId = self.faceDetectionModule.post.learnFace(p)
        self.faceDetectionModule.wait(self.learnId, 0)
        self.onSuccess()
        self.bIsRunning = False

    def onInput_onReLearn(self, p):
        if self.faceDetectionModule.reLearnFace(p):
            self.onSuccess()
        else:
            self.onFailed()
            
    def onInput_onClearDB(self):
        if self.faceDetectionModule.clearDatabase():
            self.onSuccess()
        else:
            self.onFailed()
            
    def onInput_onStop(self):
        if self.bIsRunning:
            self.faceDetectionModule.stop(self.learnId)
            self.bIsRunning = False]]>
