    def onInput_onStop(self):
        tmpMenu = self.getEmptyMenu()
        tmpMenu[0].append(self.menu[0])
        tmpMenu[1] = self.menu[1]
        tmpMenu[2] = self.menu[2]
        self.onStopped(tmpMenu)]]>
