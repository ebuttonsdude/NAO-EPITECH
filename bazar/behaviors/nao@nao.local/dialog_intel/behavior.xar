                                                                                        <script language="4">
                                                                                            <content>
                                                                                                <![CDATA[class MyClass(GeneratedClass):
	def __init__(self):
		GeneratedClass.__init__(self)

	def onInput_onStart(self, p):
		if(p == ("Back")):
			self.output_1(p)
		elif(p == ("Knee")):
			self.output_2(p)
		elif(p == ("Frog")):
			self.output_3(p)
		elif(p == ("Belly")):
			self.output_4(p)
		else:
			self.onDefault()]]>
</content>
