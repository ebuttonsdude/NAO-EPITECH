import threading

class MyClass(GeneratedClass):
  def __init__(self):
    GeneratedClass.__init__(self)
    self.tb = False
  
  def onUnload(self):
    self.onInput_onStop()
    
  def triggerOutput(self, rPeriod = 1):
    self.onStopped()
    # To clean everything
    if self.getParameter("loop"):
        self.onInput_onStart()
    else:
        self.onInput_onStop()
  
  def onInput_onStart(self):
    nMin = self.getParameter("minimum")
    nMax = self.getParameter("maximum")
    nVal = random.randint(nMin, nMax)
    self.log("wait for %dsec" % nVal)
    # Stop any previous running timer
    if self.tb:
      self.tb.cancel()
    
    self.tb = threading.Timer( nVal, self.triggerOutput, [nVal] );
    self.tb.start()
  
  def onInput_onStop(self):
    if self.tb:
      self.tb.cancel()
      self.tb = False]]>
