                                                                            <![CDATA[import random

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.nDelay = self.getParameter("delay")
        self.nFading = self.getParameter("fading")
        self.aLeds = ["FaceLed%s%d" % (side, i) for side in ["Left", "Right"] for i in range(8)]
        self.aLeds.remove("FaceLedLeft0")
        self.bMustStop = False
        self.bIsRunning = False

    def onLoad(self):
        self.bMustStop = False;
        self.bIsRunning = False
        
    def onUnload(self):
        self.onInput_onStop()
        
    def randomLedsColor(self):
        for led in self.aLeds:
            ALLeds.post.fadeRGB( led, 256*random.randint(0,255) + 256*256*random.randint(0,255) + random.randint(0,255), self.nFading)
        ALLeds.fadeRGB( "FaceLedLeft0", 256*random.randint(0,255) + 256*256*random.randint(0,255) + random.randint(0,255), self.nFading)
        
    def onInput_onStart(self):
        if self.bIsRunning:
            return
        self.bIsRunning = True

        while not self.bMustStop:   
            # Color up blinking led in one color
            self.randomLedsColor()
            # Then wait a little
            time.sleep(self.nDelay)
           
        self.bMustStop = False
        self.bIsRunning = False

    def onInput_onStop(self):
        self.bMustStop = True
        ALLeds.fadeRGB( "FaceLeds", 0xFFFFFF, self.nFading)]]>
