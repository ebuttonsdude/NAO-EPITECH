		if(p == ("Back")):
			self.output_1(p)
		elif(p == ("Knee")):
			self.output_2(p)
		elif(p == ("Frog")):
			self.output_3(p)
		elif(p == ("Belly")):
			self.output_4(p)
		else:
			self.onDefault()]]>
