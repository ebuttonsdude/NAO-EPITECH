        while not self.bMustStop:
            self.bIsRunning = True
            # Set all leds off
            ALLeds.fade("BrainLeds", 0.0, self.nFading)
            #~ ALLeds.off("BrainLeds")
            time.sleep(self.nFading)
            ALLeds.fade("BrainLeds", 1.0, self.nFading)
            #~ ALLeds.on("BrainLeds")
            time.sleep(self.nFading)

        ALLeds.fade("BrainLeds", 1.0, self.nFading)
        self.bIsRunning = False
        self.bMustStop = False
        
    def onInput_onStop(self):
        self.bMustStop = True]]>
