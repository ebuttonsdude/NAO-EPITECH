		elif(p == ("Stand")):
			self.output_2(p)
		elif(p == ("Crouch")):
			self.output_3(p)
		elif(p == ("Knee")):
			self.output_4(p)
		elif(p == ("Frog")):
			self.output_5(p)
		elif(p == ("Belly")):
			self.output_6(p)
		elif(p == ("Left")):
			self.output_7(p)
		elif(p == ("Right")):
			self.output_8(p)
		else:
			self.onDefault()]]>
