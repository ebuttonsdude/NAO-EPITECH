            self.bm.preloadBehavior(behavior)
            self.onLoaded()
        except:
            self.log("Behavior %s is already loaded" % behavior)
            self.onLoaded()
                
    def onInput_onPreload(self):
        self.preload(self.getParameter("Behavior"))
        
    def onInput_onPlay(self):
        self.stop()
        self.sBehaviorName = self.getParameter("Behavior")
        self.preload(self.sBehaviorName)        
        self.bm.runBehavior(self.sBehaviorName)
        self.sBehaviorName = None
        self.onStopped()
        
    def onInput_onStop(self):
        self.stop()
        self.onStopped()]]>
