        ear = ALLeds.post.fade("EarLeds", int/100., dur);
        self.ids.append(ear)
        ALLeds.wait(ear, 0);
        self.ids.remove(ear);
        
    def onInput_InitASR_onStart(self):
        # Set Leds OFF:
        ear = ALLeds.post.fade("EarLeds", 0.1/100., 0.01);
        self.ids.append(ear)
        ALLeds.wait(ear, 0);
        self.ids.remove(ear);
        eye = ALLeds.post.fade("FaceLeds", 0.1/100., 0.01);
        self.ids.append(eye)
        ALLeds.wait(eye, 0);
        self.ids.remove(eye);
        
        timeWaitMin = self.getParameter("timeWaitMin");
        timeWaitMax = self.getParameter("timeWaitMax");
        if (timeWaitMin > timeWaitMax):
            timeWait = time.time() + random.uniform(3.5, 5.0);
        else:
            timeWait = time.time() + random.uniform(timeWaitMin, timeWaitMax);
        
        while( timeWait > time.time() ):
            rRandTime = random.uniform(0.0,0.8)
            ALLeds.fadeRGB("FaceLeds", 256*random.randint(0,255) + 256*256*random.randint(0,255) + random.randint(0,255), rRandTime)
            time.sleep(random.uniform(0.0 ,1.0))
